{
  "name": "ethereum-block-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend for Ethereum block dashboard",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && prisma migrate deploy && tsc",
    "postinstall": "prisma generate",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:run": "vitest run",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "pg": "^8.8.0",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "ws": "^8.12.1",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^24.10.2",
    "@types/pg": "^8.6.6",
    "@types/pino": "^7.0.4",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.4",
    "typescript": "^5.3.3",
    "vitest": "^4.0.15"
  }
}